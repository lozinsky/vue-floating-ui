{
  "name": "vue-floating-ui",
  "version": "0.1.0",
  "description": "Floating UI for Vue",
  "keywords": [
    "tooltip",
    "popover",
    "dropdown",
    "menu",
    "popup",
    "positioning",
    "vue"
  ],
  "main": "./dist/vue-floating-ui.umd.js",
  "module": "./dist/vue-floating-ui.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/vue-floating-ui.umd.js",
      "import": "./dist/vue-floating-ui.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "./dist/"
  ],
  "author": "Pavel Lozinsky <pavel@lozinsky.dev>",
  "license": "MIT",
  "bugs": "https://github.com/lozinsky/vue-floating-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/lozinsky/vue-floating-ui.git",
    "directory": "packages/vue-floating-ui"
  },
  "homepage": "https://github.com/lozinsky/vue-floating-ui",
  "scripts": {
    "start": "cypress open --component",
    "build": "vite build",
    "test": "cypress run --component"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.4",
    "vue-demi": "^0.13.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "cypress": "^11.0.1",
    "vite": "^3.2.3",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-dts": "^1.7.0",
    "vite-tsconfig-paths": "^3.5.2",
    "vue": "^3.2.45"
  }
}
